{
  "version": 2,
  "builds": [
    {
      "src": "next.config.js",
      "use": "@vercel/node"
    },
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "env": {
    "MY_ENV_VAR": "my-value"
  },
  "build": {
    "command": "npm install && npm run build",
    "env": {
      "BUILD_ENV_VAR": "build-value"
    }
  }
}
